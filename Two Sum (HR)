#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main()
{
    int N;
    scanf("%d", &N);
    if(N<2 || N>10000)
    {
        printf("Invalid Input");
        exit(0);
    }
    int arr[N];
    for(int i=0; i<N; i++)
    {
        scanf("%d", &arr[i]);
    }
    int s;
    scanf("%d", &s);
    for(int i=0; i<N; i++)
    {
        for(int j=0; j<N; j++)
        {
            if(i==j)
            {
                continue;
            }
            if(arr[i]+arr[j]==s)
            {
                printf("%d %d", i, j);
                return 0;
            }
        }
    }
    return 0;
}
